  "scripts": {
    "test:clear": "jest --clearCache",
    "test:context": "jest test/context --detectOpenHandles",
    "test:models": "jest test/models --detectOpenHandles",
    "test:services": "jest test/services --detectOpenHandles",
    "test:controllers": "jest test/controllers --detectOpenHandles",
    "test": "npm run test:clear && npm run test:context && npm run test:models && npm run test:services && npm run test:controllers",
    "test:single": "node -r dotenv/config && jest",
    "build": "rm -rf dist && webpack",
    "start": "npm run build && node dist/app.js",
    "start:local": "npm run build && node -r dotenv/config dist/app.js"
  },
